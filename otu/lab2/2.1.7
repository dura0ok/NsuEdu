clear;
x = -10:0.1:10;
for i = 1:1:length(x)
    y(i) = ((x(i) - 4.5)^2 * (x(i) + 2))^(1/3);
end
plot(x, y);
xgrid();
// Добавление меток на оси ординат
ylabel('Y-axis');

// Добавление горизонтальной линии в нуле
plot([-10, 10], [0, 0], 'r--'); // Горизонтальная линия в нуле (от -10 до 10 по оси x, от 0 до 0 по оси y)

// Добавление вертикальной линии в нуле
plot([0, 0], [-10, 10], 'g--'); // Вертикальная линия в нуле (от 0 до 0 по оси x, от -10 до 10 по оси y)

