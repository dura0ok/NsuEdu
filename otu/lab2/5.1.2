function [zr]=G(c,z)
zr=z(2)-c(1)-c(2)*z(1)-c(3)*z(1)^2-c(4)*z(1)^3-c(5)*z(1)^4
endfunction

t=[0.66 0.9 1.17 1.47 1.7 1.74 2.08 2.63 3.12];
Zt=[38.9 68.8 64.4 66.5 64.95 59.36 82.6 90.63 113.5];

z=[t;Zt];
c=[0;0;0;0;0];
[a,err]=datafit(G,z,c);

printf("K,D,C,B,A:\n");
disp(a);
printf("err:\n");
disp(err);

plot2d(t,Zt,-4);
t=0.5:0.01:3.5;
